<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.anonymousru.thebestdevelopers.anonymousru" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="Anonymous RU"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="Anonymous RU" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CardapioActivity"
            android:label="@string/title_activity_cardapio" >
        </activity>
        <activity
            android:name=".ChatActivity"
            android:label="@string/title_activity_chat" >
        </activity>
        <activity
            android:name=".MensageActivity"
            android:label="@string/title_activity_mensage" >
        </activity>
    </application>

</manifest>
